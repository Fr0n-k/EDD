package Server;

/**
 *
 * @author fcarv
 */
public class NodoCabecera {

    String Nombre;
    String Descripcion;
    double Ponderacion;
    String Fecha_Entrega;
    String Tipo;
    int idx;
    NodoCabecera Siguiente;
    NodoCabecera Anterior;
    NodoNota Acceso;

    public NodoCabecera(String Nombre, String Descripcion, double Ponderacion, String Fecha_Entrega, String Tipo) {
        this.Nombre = Nombre;
        this.Descripcion = Descripcion;
        this.Ponderacion = Ponderacion;
        this.Fecha_Entrega = Fecha_Entrega;
        this.Tipo = Tipo;
        this.idx = 0;
        this.Siguiente = null;
        this.Anterior = null;
        this.Acceso = null;
    }
    
}
